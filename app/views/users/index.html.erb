<% provide(:title, 'All users') %>
<h1>All users</h1>

<%= form_tag users_path, :method => 'get' do %>
  <div class="row">
    <div class="span6" >
      <%= text_field_tag :search, params[:search] %>
      <%= submit_tag "Search", :name => nil,  class: "btn btn-large btn-primary" %>
    </div>
  </div>
<% end %>

<%= will_paginate %>
<ul class="users">
  <%= render @users %> 
</ul>

<%= will_paginate %>